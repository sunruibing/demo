		160. //返回值:拼音首字母串数组    
		161.
		function makePy(str) {
			162.
			if(typeof(str) != "string")
				163.
			throw new Error(-1, "函数makePy需要字符串类型参数!");
			164.
			var arrResult = new Array(); //保存中间结果的数组    
			165.
			for(var i = 0, len = str.length; i < len; i++) {
				166. //获得unicode码    
				167.
				var ch = str.charAt(i);
				168. //检查该unicode码是否在处理范围之内,在则返回该码对映汉字的拼音首字母,不在则调用其它函数处理    
				169. arrResult.push(checkCh(ch));
				170.
			}
			171. //处理arrResult,返回所有可能的拼音首字母串数组    
			172.
			return mkRslt(arrResult);
			173.
		}
		174.
		function checkCh(ch) {
			175.
			var uni = ch.charCodeAt(0);
			176. //如果不在汉字处理范围之内,返回原字符,也可以调用自己的处理函数    
			177.
			if(uni > 40869 || uni < 19968)
				178.
			return ch; //dealWithOthers(ch);    
			179. //检查是否是多音字,是按多音字处理,不是就直接在strChineseFirstPY字符串中找对应的首字母    
			180.
			return(oMultiDiff[uni] ? oMultiDiff[uni] : (strChineseFirstPY.charAt(uni - 19968)));
			181.
		}
		182.
		function mkRslt(arr) {
			183.
			var arrRslt = [""];
			184.
			for(var i = 0, len = arr.length; i < len; i++) {
				185.
				var str = arr[i];
				186.
				var strlen = str.length;
				187.
				if(strlen == 1) {
					188.
					for(var k = 0; k < arrRslt.length; k++) {
						189. arrRslt[k] += str;
						190.
					}
					191.
				} else {
					192.
					var tmpArr = arrRslt.slice(0);
					193. arrRslt = [];
					194.
					for(k = 0; k < strlen; k++) {
						195. //复制一个相同的arrRslt    
						196.
						var tmp = tmpArr.slice(0);
						197. //把当前字符str[k]添加到每个元素末尾    
						198.
						for(var j = 0; j < tmp.length; j++) {
							199. tmp[j] += str.charAt(k);
							200.
						}
						201. //把复制并修改后的数组连接到arrRslt上    
						202. arrRslt = arrRslt.concat(tmp);
						203.
					}
					204.
				}
				205.
			}
			206.
			return arrRslt;
			207.
		}
